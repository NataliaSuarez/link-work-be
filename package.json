    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.1.2",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.1.2",
    "@sendgrid/mail": "^7.7.0",
    "@typegoose/typegoose": "^9.12.0",
    "apple-auth": "^0.0.1",
    "argon2": "0.29.1",
