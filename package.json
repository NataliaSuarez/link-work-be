  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations:generate": "npm run typeorm -- migration:generate -d ./src/database/data-source.ts",
    "migrations:create": "npm run typeorm -- migration:create",
    "migrations:run": "npm run typeorm -- migration:run -d ./src/database/data-source.ts",
    "migrations:show": "npm run typeorm -- migration:show -d ./src/database/data-source.ts",
    "migrations:revert": "npm run typeorm -- migration:revert -d ./src/database/data-source.ts",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/database/data-source.ts"
  },
  "dependencies": {
    "@arendajaelu/nestjs-passport-apple": "1.0.3",
